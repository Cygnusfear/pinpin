{
  "mcpServers": {
    "mastra": {
      "command": "bunx",
      "args": [
        "-y",
        "@mastra/mcp-docs-server"
      ],
      "cwd": "/Users/alexander/Node/tonk/template-test/pinpin",
      "alwaysAllow": [
        "mastraDocs",
        "mastraExamples"
      ]
    },
    "pinpin-store": {
      "command": "node",
      "args": [
        "server/src/mcpServer.ts"
      ],
      "cwd": "/Users/alexander/Node/tonk/template-test/pinpin",
      "alwaysAllow": [
        "view_all_pinboard_widgets",
        "view_widget_content",
        "view_pinboard_ui_state",
        "add_pinboard_widget",
        "update_pinboard_widget",
        "update_widget_content",
        "remove_pinboard_widget"
      ]
    }
  },
  "permissions": {
    "allow": [
      "mcp__pinpin-store__view_all_pinboard_widgets",
      "mcp__pinpin-store__add_pinboard_widget",
      "mcp__pinpin-store__view_widget_content"
    ],
    "deny": []
  },
  "enableAllProjectMcpServers": true
}